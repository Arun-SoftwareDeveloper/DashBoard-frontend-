<script>
/* below script added to redirect to login page on adding product to wishlist if not logged in */
var wishlist= {
    'add':function(product_id) {
        $.ajax( {
            url:'index.php?route=account/wishlist/add', type:'post', data:'product_id='+product_id+'&__csrf='+$('#__csrf').val(), dataType:'json', success:function(json) {
                if(json.hasOwnProperty('error') && json['error'] != "") {
                    $('#content, .common-home .breadcrumb').parent().before('<div class="alert alert-warning cart-added-success-message"><i class="fa fa-check-circle"></i> ' + json['error'] + ' <button type="button" class="close" data-dismiss="alert">&times;</button></div>');
                    setTimeout(function () {
                            $('.cart-added-success-message').fadeOut('slow');
                    }, 1000);
                    return false;
                }
                if(json.success) {
                    $(".wishlist-add-"+product_id).addClass("active");
                    $(".wishlist-add-"+product_id).addClass("wishlist-remove-"+product_id);
                    $(".wishlist-add-"+product_id).attr('onClick', "wishlist.remove("+product_id+")");
                    $(".wishlist-add-"+product_id).attr('data-original-title', "Remove from Wishlist");
                    $(".wishlist-add-"+product_id).removeClass("wishlist-add-"+product_id);
                    $(".wishlist-span-"+product_id).text("Remove from Wishlist");
                    $('#content').parent().before('<div class="alert alert-success wishlist-success-message"><i class="fa fa-check-circle"></i> ' + json['success'] + ' <button type="button" class="close" data-dismiss="alert">&times;</button></div>');
                    setTimeout(function () {
                        $('.wishlist-success-message').fadeOut('slow');
                    }, 1000);
                    $('span#wishlist-total').html(json.total_products);
                    $('span#wishlist-total').css("display","block");
                }              
                if(json.redirect_login) {
                    document.location.href=json.login_link;
                }
            }
        }
        )
    }
    ,
    'remove':function(product_id) {
        $.ajax( {
                url:'index.php?route=account/wishlist/remove', type:'post', data:'product_id='+product_id+'&__csrf='+$('#__csrf').val(), dataType:'json', success:function(json) {
                    if(json.hasOwnProperty('error') && json['error'] != "") {
                        $('#content, .common-home .breadcrumb').parent().before('<div class="alert alert-warning cart-added-success-message"><i class="fa fa-check-circle"></i> ' + json['error'] + ' <button type="button" class="close" data-dismiss="alert">&times;</button></div>');
                        setTimeout(function () {
                            $('.cart-added-success-message').fadeOut('slow');
                        }, 1000);
                        return false;
                    }
                    if(json.success) {
                        $(".wishlist-remove-"+product_id).addClass("wishlist-add-"+product_id);
                        $(".wishlist-remove-"+product_id).attr('onClick', "wishlist.add("+product_id+")");
                        $(".wishlist-remove-"+product_id).attr('data-original-title',  "Add to Wishlist");
                        $(".wishlist-remove-"+product_id).removeClass("active");
                        $(".wishlist-remove-"+product_id).removeClass("wishlist-remove-"+product_id);
                        $(".wishlist-span-"+product_id).text("Add to Wishlist");
                        $('#content').parent().before('<div class="alert alert-success wishlist-success-message"><i class="fa fa-check-circle"></i> ' + json['success'] + ' <button type="button" class="close" data-dismiss="alert">&times;</button></div>');
                        setTimeout(function () {
                            $('.wishlist-success-message').fadeOut('slow');
                        }, 1000);
                        $('span#wishlist-total').html(json.total_products);
                        $('span#wishlist-total').css("display","block");
                    }              
                    if(json.redirect_login) {
                        document.location.href=json.login_link;
                    }
                }
            }
          )
    }       
}

</script>
<script>
$(document).ready(function() {
  islPromotionsPing(100);

  // Instead of watch addCart button, monitor the xhr
  $(document).ajaxComplete(function(e, xhr, settings) {
    if (xhr.status != 200) {
      return;
    }
    if (settings.url.indexOf('checkout/cart/add') != -1 || settings.url.indexOf('checkout/cart/remove') != -1) {
      islPromotionsPing(300, {"ntf_success" : true});
    }
  });
});
function islPromotionsPing($timeout, $param) {
  if ($param == undefined) {
    $param = [];
  }

  $('.islpr-alert').remove();
  setTimeout(function() {
    $.ajax({
      url      : 'index.php?route=extension/module/promotions/ping',
      type     : 'post',
      dataType : 'json',
      data     : $param,
      success: function(data) {
        if (data) {
          $.each(data, function(key, value) {
            $('#content').prepend('<div class="alert alert-' + value.alert + ' islpr-alert islpr-alert-' + value.theme + '">' + value.message + ' <button type="button" class="close" data-dismiss="alert">×</button></div>');
          });
        }
      }
    });
  }, $timeout);
}
</script>
