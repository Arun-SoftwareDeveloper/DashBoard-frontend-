function submitchkzipcodform(form,flag='') {
    if(validationcheck()) {
        var product_size = $('#product input[type=\'radio\']:checked').val();
        
        $.ajax({
            url: 'https://www.jackjones.in/index.php?route=extension/module/clickpost/checkzipcode',
            type: 'post',
            data: {'postcode_check': $('#postcode_check').val(), 'product_size': product_size},//product_size => product_option_value_id
            dataType: 'json',
            beforeSend: function() {
            	$('#chkzipcod_response').html('<img src="image/loader.gif"/>');
            }, 
            success: function(json) {
                if (json['code'] == 200) {
                    var class_added = $('#product .required');
                    if(flag == 1 && !class_added.hasClass('has-error')) {
                        $('#product .required').addClass('has-error');
                        $('#product .required').append('<div class="text-danger">Size required!</div>');
                        if ($(window).width() < 992) {
                            $("html, body").animate({ scrollTop: $("h1.product-title").offset().top }, "slow");
                        }else {
                            $('html, body').animate({ scrollTop: 0 }, 'slow');
                        }
                    } else if (flag == 1 && class_added.hasClass('has-error')) {
                        if ($(window).width() < 992) {
                            $("html, body").animate({ scrollTop: $("h1.product-title").offset().top }, "slow");
                        }else {
                            $('html, body').animate({ scrollTop: 0 }, 'slow');
                        }
                    }
                    $('#chkzipcod_response').html(json['success']);
                    
                } else if(json['code'] == 400) { // location not serviceable
                    $('#chkzipcod_response').html(json['success']);
                } else if(json['code'] == 202) {//202 is used to differentiate success response
                    $('#chkzipcod_response').html(json['success']);
                    if(json['success'].indexOf("Out of Stock") > 1) {
                        $('.myalert_chkzipcod').addClass('no-stock');
                    }
                } 
            },
            error: function(xhr, ajaxOptions, thrownError) {
                alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
            }
        });
    }
} 

function validationcheck() {       
        var postcode = $('#postcode_check').val();
        var reg = new RegExp('^[1-9][0-9]{5}$');
        $('#message').remove();
        $('#chkzipcod_response1').css('display','block');
        if ((postcode.length)< 6 || (postcode.length) > 6 ){
            $('#chkzipcod_response').html('');
            $('#chkzipcod_response1').html("<p id='message' class='myalert_chkzipcod danger'>zipcode should only be 6 digits</p>").delay(3000).fadeOut("slow");
            return false;
        }
        else if (reg.test(postcode) == false){
            $('#chkzipcod_response').html('');
            $('#chkzipcod_response1').html("<p id='message' class='myalert_chkzipcod danger'>zipcode should be numbers between 1 - 9</p>").delay(3000).fadeOut("slow");
            return false;
        }
       else {
          return true;
       }
    }

